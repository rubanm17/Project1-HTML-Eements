DROP TABLE IF EXISTS C_S;

CREATE TABLE C_S {
    SNO INTEGER PRIMARY KEY NOT NULL,
    Roll_NO INTEGER NOT NULL,
    Name TEXT NOT NULL,
    AGE INTEGER DEFAULT 15,
    GENDER TEXT NOT NULL,
    Email_ID TEXT NOT NULL,
    Contact_No REAL NOT NULL
};

